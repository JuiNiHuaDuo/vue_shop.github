    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "better-scroll": "^1.15.2",
    "core-js": "^3.8.3",
    "mint-ui": "2.2",
    "run": "^1.4.0",
    "v-distpicker": "1.2",
    "vue": "^2.6.14",
    "vue-preview": "^1.1.3",
    "vue-router": "3.1",
    "vuex": "3.1"
